{"ast":null,"code":"import \"core-js/modules/es.function.name.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n// 组件交互相关的代码(数据、方法)\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Student',\n  data: function data() {\n    return {\n      name: '张三',\n      sex: '男',\n      number: 0\n    };\n  },\n  // export{school}\n  // export default school\n  methods: {\n    sendStudentName: function sendStudentName() {\n      //触发Student组件实例上的atguigu事件\n      this.$emit('atguigu', this.name, 666, 888, 900);\n      this.$emit('demo');\n    },\n    unbind: function unbind() {\n      // this.$off('atguigu')//解绑一个自定义事件\n      // this.$off(['atguigu','demo'])//解绑一多个自定义事件\n      //this.$off()//解绑所有的自定义事件\n      console.log('解绑');\n    },\n    death: function death() {\n      this.$destroy(); //销毁当前Student组件的实例\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;AAcA;AACA;AACA;AACAA,iBAFA;AAGAC,MAHA,kBAGA;AACA;AACAD,gBADA;AAEAE,cAFA;AAGAC;AAHA;AAKA,GATA;AAUA;AACA;AACAC;AACAC,mBADA,6BACA;AACA;AACA;AACA;AAEA,KANA;AAOAC,UAPA,oBAOA;AACA;AACA;AACA;AACAC;AACA,KAZA;AAaAC,SAbA,mBAaA;AACA,sBADA,CACA;AACA;AAfA;AAZA","names":["name","data","sex","number","methods","sendStudentName","unbind","console","death"],"sourceRoot":"src/components","sources":["Student.vue"],"sourcesContent":["<template>\r\n    <!-- 组件的结构 -->\r\n    <div class=\"student\">\r\n        <h2>学生名称: {{name}}</h2>\r\n        <h2>学生性别: {{sex}}</h2>\r\n        <h2>当前求和为:{{number}}</h2>\r\n        <button @click=\"add\"></button>\r\n        <button @click=\"sendStudentName\">点我把学生名给APP</button>\r\n        <button @click=\"unbind\">解绑atguigu事件</button>\r\n        <button @click=\"death\">销毁当前Student组件的实例(vc)</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    // 组件交互相关的代码(数据、方法)\r\n    export default {\r\n    // eslint-disable-next-line vue/multi-word-component-names\r\n        name: 'Student',\r\n        data(){\r\n            return {\r\n                name: '张三',\r\n                sex: '男',\r\n                number: 0\r\n            }\r\n        },\r\n        // export{school}\r\n        // export default school\r\n        methods: {\r\n            sendStudentName(){\r\n                //触发Student组件实例上的atguigu事件\r\n                this.$emit('atguigu',this.name,666,888,900)\r\n                this.$emit('demo')\r\n\r\n            },\r\n            unbind(){\r\n                // this.$off('atguigu')//解绑一个自定义事件\r\n                // this.$off(['atguigu','demo'])//解绑一多个自定义事件\r\n                //this.$off()//解绑所有的自定义事件\r\n                console.log('解绑');\r\n            },\r\n            death(){\r\n                this.$destroy()//销毁当前Student组件的实例\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n\r\n<style>\r\n    /* 组件的样式 */\r\n    .student{\r\n        background-color:orange;\r\n        padding: 5px;\r\n        margin-top: 30px;\r\n    }\r\n</style>\r\n\r\n\r\n"]},"metadata":{},"sourceType":"module"}
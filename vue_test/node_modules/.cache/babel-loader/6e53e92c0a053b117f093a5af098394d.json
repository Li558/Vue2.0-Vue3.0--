{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//引入组件\nimport School from './components/School';\nimport Student from './components/Student';\nexport default {\n  name: 'App',\n  components: {\n    School: School,\n    Student: Student\n  },\n  data: function data() {\n    return {\n      msg: '你好啊'\n    };\n  },\n  methods: {\n    getSchoolName: function getSchoolName(name) {\n      console.log('App收到了学校名字: ', name);\n    },\n    getStudentName: function getStudentName(name, x, y, z) {\n      console.log('App收到了学生名字', name, x, y, z);\n    }\n  },\n  //第二种\n  mounted: function mounted() {\n    this.$refs.student.$once('atguigu', this.getStudentName);\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;AAeA;AACA;AACA;AACA;AACAA,aADA;AAEAC;AACAC,kBADA;AAEAC;AAFA,GAFA;AAMAC,MANA,kBAMA;AACA;AACAC;AADA;AAGA,GAVA;AAWAC;AACAC,iBADA,yBACAP,IADA,EACA;AACAQ;AACA,KAHA;AAIAC,kBAJA,0BAIAT,IAJA,EAIAU,CAJA,EAIAC,CAJA,EAIAC,CAJA,EAIA;AACAJ;AAEA;AAPA,GAXA;AAoBA;AACAK,SArBA,qBAqBA;AACA;AACA;AAvBA","names":["name","components","School","Student","data","msg","methods","getSchoolName","console","getStudentName","x","y","z","mounted"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\r\n  <div class=\"app\">\r\n    <h1>{{msg}}</h1>\r\n    <!-- 通过父组件给子组件传递函数类型的props实现: 子给父传递数据 -->\r\n    <School :getSchoolName=\"getSchoolName\"></School>\r\n    <!-- 通过父组件给子组件绑定一个自定义事件实现: 子给父传递数据(第一种写法，使用@或v-on) -->\r\n    <!-- <Student v-on:atguigu.once = \"getStudentName\"></Student> -->\r\n\r\n    <!-- 通过父组件给子组件绑定一个自定义事件实现: 子给父传递数据(第二种ref) -->\r\n    <Student ref=\"student\"/>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n//引入组件\r\nimport School from './components/School'\r\nimport Student from './components/Student'\r\nexport default {\r\n  name: 'App',\r\n  components:{\r\n      School,\r\n      Student\r\n  },\r\n    data(){\r\n      return {\r\n        msg: '你好啊'\r\n      }\r\n    },\r\n    methods:{\r\n      getSchoolName(name){\r\n        console.log('App收到了学校名字: ', name);\r\n      },\r\n      getStudentName(name,x,y,z){\r\n        console.log('App收到了学生名字',name,x,y,z);\r\n\r\n      }\r\n    },\r\n    //第二种\r\n    mounted(){\r\n        this.$refs.student.$once('atguigu', this.getStudentName)\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n  .app{\r\n    background-color:gray;\r\n    padding: 5px;\r\n  }\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}